extends layout
block dashBord
  #page-wrapper
    .container-fluid
      // Page Heading
      .row
        .col-lg-12
          h1.page-header  Admin Serial Number 
            small  Perpaid Card
          ol.breadcrumb
            li
              i.fa.fa-dashboard
              a(href='/adminPage')  Dashboard
            li.active
              i.glyphicon.glyphicon-check
              |  Perpaid Card
          .col-md-10.col-md-offset-1
            .tile-progress.tile-primary
              .tile-header
                h4 العدد الكلي للكروت : #{cardNumber} كرت
                h4 المبلغ الكلي للكروت : #{mony} د.ل
                h4 فئة 20 : العدد #{twentyMony[0].c} كرت , المبلغ #{twentyMony[0].s} د.ل
                h4 فئة 50 : العدد #{fmony[0].c} كرت , المبلغ #{fmony[0].s} د.ل
                h4 فئة 100: العدد #{hmony[0].c} كرت , المبلغ #{hmony[0].s} د.ل
              .tile-progressbar
                span(data-fill='10%', style='width: 100%;')
              .tile-footer
                h4
                  span.pct-counter 100
                  | % النسبة
                span الاحصائيات
            .tile-progress.tile-red
              .tile-header
                h4 عدد الكروت المستخدة : 1150
                h4 فئة 10 : 3000
                h4 فئة 40 : 4000
                h4 فئة 100: 1000
              .tile-progressbar
                span(data-fill='11.5%', style='width: 11.5%;')
              .tile-footer
                h4
                  span.pct-counter 11.5
                  | % النسبة
                span الحصائيات 
            .tile-progress.tile-blue
              .tile-header
                h4 عدد الكروت الغير مستخدمة : 8850 
                h4 فئة 10 : 3000
                h4 فئة 40 : 4000
                h4 فئة 100: 1000
              .tile-progressbar
                span(data-fill='88.5%', style='width: 88.5%;')
              .tile-footer
                h4
                  span.pct-counter 88.5
                  | % النسبة
                span الحصائيات
            .tile-progress.tile-aqua
              .tile-header
                h4 المبلغ المستخدم : 33000 د.ل  
                h4 فئة 10 : 3000 د.ل 
                h4 فئة 40 : 4000 د.ل
                h4 فئة 100: 1000 د.ل
              .tile-progressbar
                span(data-fill='22%', style='width: 22%;')
              .tile-footer
                h4
                  span.pct-counter 22
                  | % النسبة 
                span الحصائيات
            .tile-progress.tile-green
              .tile-header
                h4 المبلغ الغير مستخدم : 55000 د.ل
                h4 فئة 10 : 3000 د.ل 
                h4 فئة 40 : 4000 د.ل
                h4 فئة 100: 1000 د.ل 
              .tile-progressbar
                span(data-fill='88%', style='width: 88%;')
              .tile-footer
                h4
                  span.pct-counter 88
                  | % النسبة
                span الحصائيات
  script(src='/js/jquery-1.11.2.min.js')
  script(src='/js/bootstrap.min.js')
  script(src='/js/adminSerialNumber.js')
  script(src='metisMenu/dist/metisMenu.min.js')
  script(src='/js/sb-admin-2.js')


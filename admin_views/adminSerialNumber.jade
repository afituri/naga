extends layout
block dashBord
  #page-wrapper
    .container-fluid
      // Page Heading
      .row
        .col-lg-12
          h1.page-header  Admin Serial Number 
            small  Perpaid Card
          ol.breadcrumb
            li
              i.fa.fa-dashboard
              a(href='/adminPage')  Dashboard
            li.active
              i.glyphicon.glyphicon-check
              |  Perpaid Card
          .col-md-10.col-md-offset-1
            .tile-progress.tile-primary
              .tile-header
                h4 العدد الكلي للكروت : #{cardNumber} كرت
                h4 مبلغ المال الكلي للكروت : #{mony} د.ل
                h4 فئة 20 : العدد #{twentyMony[0].c} كرت , المبلغ #{twentyMony[0].s} د.ل
                h4 فئة 50 : العدد #{fmony[0].c} كرت , المبلغ #{fmony[0].s} د.ل
                h4 فئة 100: العدد #{hmony[0].c} كرت , المبلغ #{hmony[0].s} د.ل
              .tile-progressbar
                span(data-fill='10%', style='width: 100%;')
              .tile-footer
                h4
                  span.pct-counter 100
                  | % النسبة
                span النسبة الكاملة
            .tile-progress.tile-red
              .tile-header
                h4 عدد الكروت المستخــدمة : #{all[0].c} كرت
                h4 مبلغ المال للكروت المستخدمة : #{all[0].s} 
                h4 فئة 20 : العدد #{Tused} كرت , المبلغ #{TTused} د.ل
                h4 فئة 50 : العدد #{Fused} كرت , المبلغ #{FFused} د.ل
                h4 فئة 100: العدد #{Hused} كرت , المبلغ #{HHused} د.ل
              .tile-progressbar
                
                span(data-fill='#{usedPercent}%', style='width: #{usedPercent}%;')
              .tile-footer
                h4
                  span.pct-counter #{usedPercent}
                  | % النسبة
                span تحديد نسبة الكروت المستخدمة حسب  العدد
            .tile-progress.tile-blue
              .tile-header
                h4 عدد الكروت الغير المستخــدمة : #{notUsed} كرت
                h4 مبلغ المال للكروت الغير المستخدمة : #{mony - all[0].s}
                h4 فئة 20 : العدد #{twentyMony[0].c - Tused} كرت , المبلغ #{twentyMony[0].s - TTused} د.ل
                h4 فئة 50 : العدد #{fmony[0].c - Fused} كرت , المبلغ #{fmony[0].s - FFused} د.ل
                h4 فئة 100: العدد #{hmony[0].c - Hused} كرت , المبلغ #{hmony[0].s - HHused} د.ل
              .tile-progressbar
                span(data-fill='#{100-usedPercent}%', style='width: #{100-usedPercent}%;')
              .tile-footer
                h4
                  span.pct-counter #{100-usedPercent}
                  | % النسبة
                span تحديد نسبة الكروت الغير مستخدمة حسب العدد 
            .tile-progress.tile-aqua
              .tile-header
                h4 مبلغ المال sلكروت المستخدمة : #{TTused+FFused+HHused} د.ل  
                h4 فئة 20 : #{TTused} د.ل 
                h4 فئة 50 : #{FFused} د.ل
                h4 فئة 100: #{HHused} د.ل
              .tile-progressbar
                span(data-fill='#{(TTused+FFused+HHused)*(100/mony)}%', style='width: #{(TTused+FFused+HHused)*(100/mony)}%;')
              .tile-footer
                h4
                  span.pct-counter #{(TTused+FFused+HHused)*(100/mony)}
                  | % النسبة 
                span تحديد نسبة الكروت المستخدمة حسب المبلغ المالي
            .tile-progress.tile-green
              .tile-header
                h4 مبلغ الكروت الغير مستخدمة: #{mony - all[0].s} د.ل
                h4 فئة 20 : #{twentyMony[0].s - TTused} د.ل 
                h4 فئة 50 : #{fmony[0].s - FFused} د.ل
                h4 فئة 100: #{hmony[0].s - HHused} د.ل 
              .tile-progressbar
                span(data-fill='#{100-(TTused+FFused+HHused)*(100/mony)}%', style='width: #{100-(TTused+FFused+HHused)*(100/mony)}%;')
              .tile-footer
                h4
                  span.pct-counter #{100-(TTused+FFused+HHused)*(100/mony)}
                  | % النسبة
                span تحديد نسبة الكروت الغير مستخدمة حسب المبلغ المالي
  script(src='/js/jquery-1.11.2.min.js')
  script(src='/js/bootstrap.min.js')
  script(src='/js/adminSerialNumber.js')
  script(src='metisMenu/dist/metisMenu.min.js')
  script(src='/js/sb-admin-2.js')


extends layout
block Dashboard
  #page-wrapper
    .container-fluid
      // Page Heading
      .row
        h1  Admin Manger
          small  View Admins
        ol.breadcrumb
          li
            i.fa.fa-dashboard
            a(href='/adminPage')  Dashboard
          li.active
            i.fa.fa-user  vendor
        .row    
          .col-xs-5.pull-right
            .form-group      
              .h5(style ="font-weight: bold; color:#FF9900 ;") Is It Company !
              input#discount_flag(type='checkbox', name='discount_flag')    
        .row        
          .col-md-8.col-md-offset-2
                form.form-horizontal#Hide1(role='form')
                  .table-responsive
                    table.table.table-striped.table-condensed
                      thead
                        tr
                          td.text-center
                            strong Name in Arabic
                          td.text-center
                            strong Name in English  
                          td.text-center
                            strong Edit  
                          td.text-center
                            strong Trash 
                      tbody#colorTable
                       
                          tr(data-id='')
                            td.text-center 
                               a.editable.editable-click(id='name', href='#', data-type='text', data-pk='') 
                            td.text-center
                              a.editable.editable-click(id='name_en', href='#', data-type='text', data-pk='')  
                            td.text-center
                              button#enable.btn.btn-info.btn-xs(value='' ,data-title='Edit', data-toggle='modal')
                                span.glyphicon.glyphicon-pencil     
                            td.text-center
                              button#delete.btn.btn-danger.btn-xs(href='', data-toggle='modal',value='')
                                span.glyphicon.glyphicon-trash   
                      
                form.form-horizontal#Hide(role='form')
                  .table-responsive
                    table.table.table-striped.table-condensed
                      thead
                        tr

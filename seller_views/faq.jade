    body
      #header
        .container
          #welcomeLine.row
            .span6
              | Welcome!
              strong  User
            .span6
              .pull-right
                a(href='product_summary.html')
                  span Fr
                a(href='product_summary.html')
                  span Es
                span.btn.btn-mini En
                a(href='product_summary.html')
                  span £
                span.btn.btn-mini $155.00
                a(href='product_summary.html')
                  span $
                a(href='product_summary.html')
                  span.btn.btn-mini.btn-primary
                    i.icon-shopping-cart.icon-white
                    |  [ 3 ] Itemes in your cart
          // Navbar ==================================================
          #logoArea.navbar
            a#smallScreen.btn.btn-navbar(data-target='#topMenu', data-toggle='collapse')
              span.icon-bar
              span.icon-bar
              span.icon-bar
            .navbar-inner
              a.brand(href='index.html')
                img(src='themes/images/logo.png', alt='Bootsshop')
              form.form-inline.navbar-search(method='post', action='products.html')
                input#srchFld.srchTxt(type='text')
                select.srchTxt
                  option All
                  option CLOTHES 
                  option FOOD AND BEVERAGES 
                  option HEALTH & BEAUTY 
                  option SPORTS & LEISURE 
                  option BOOKS & ENTERTAINMENTS 
                button#submitButton.btn.btn-primary(type='submit') Go
              ul#topMenu.nav.pull-right
                li
                  a(href='special_offer.html') Specials Offer
                li
                  a(href='normal.html') Delivery
                li
                  a(href='contact.html') Contact
                li
                  a(href='#login', role='button', data-toggle='modal', style='padding-right:0')
                    span.btn.btn-large.btn-success Login
                  #login.modal.hide.fade.in(tabindex='-1', role='dialog', aria-labelledby='login', aria-hidden='false')
                    .modal-header
                      button.close(type='button', data-dismiss='modal', aria-hidden='true') ×
                      h3 Login Block
                    .modal-body
                      form.form-horizontal.loginFrm
                        .control-group
                          input#inputEmail(type='text', placeholder='Email')
                        .control-group
                          input#inputPassword(type='password', placeholder='Password')
                        .control-group
                          label.checkbox
                            input(type='checkbox')
                            |  Remember me
                      button.btn.btn-success(type='submit') Sign in
                      button.btn(data-dismiss='modal', aria-hidden='true') Close

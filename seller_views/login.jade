extends layout
block content
  .col-xs-8.col-xs-offset-2.col-sm-6.col-sm-offset-3.col-md-4.col-md-offset-4
    a(href="/adminLogin")
      img#img-responsive(src="../img/nagaLogo1.png")
    .panel.panel-warning    
      .panel-body  
        .row
           .col-md-8.col-md-offset-1
             h3#h5Login Sign in
        form.form-horizontal#loginn(role="form",name="loginn")  
          br
          .form-group   
            .row
              .col-md-2.col-md-offset-2.col-sm-2.col-sm-offset-2.col-xs-3.col-xs-offset-2
                label Email
              .col-md-6.col-sm-6.col-xs-6   
                input.form-control#email(type = "email", name = "email", placeholder = "Email")  
          br
          .form-group   
            .row
              .col-md-2.col-md-offset-2.col-sm-2.col-sm-offset-2.col-xs-3.col-xs-offset-2
                label Password
              .col-md-6.col-sm-6.col-xs-6   
                input.form-control#Password(type = "Password", name = "Password", placeholder = "Password")    
          
          .form-group   
            .row
              .col-md-5.col-md-offset-1.col-sm-12.col-sm-offset-1.col-xs-12.col-xs-offset-1
                a(href='#')   Forgot your password?

            .row
              .col-md-5.col-md-offset-1.col-sm-12.col-sm-offset-1.col-xs-12.col-xs-offset-1
                a(href='#')   I am a new customer                
 
            .row         
              .col-md-12.col-md-offset-6.col-sm-12.col-sm-offset-6.col-xs-12.col-xs-offset-6
               button#Loginbt(type= "submit") Login  



                 

  //script(src='/themes/js/jquery.js')
  script(src='/js/jquery-1.11.2.min.js')
  script(src="/js/jquery.validate.min.js")  
  script(src='/js/loginUser.js') 
  script(src='/themes/js/bootstrap.min.js')
  script(src='/themes/js/google-code-prettify/prettify.js')
  script(src='/themes/js/bootshop.js')
  script(src='/themes/js/jquery.lightbox-0.5.js')  
     

              li
                a(href='#login', role='button', data-toggle='modal', style='padding-right:0')
                  span.btn.btn-large.btn-success Login
                #login.modal.hide.fade.in(tabindex='-1', role='dialog', aria-labelledby='login', aria-hidden='false')
                  .modal-header
                    button.close(type='button', data-dismiss='modal', aria-hidden='true') ×
                    h3 Login Block
                  .modal-body
                    form.form-horizontal.loginFrm
                      .control-group
                        input#inputEmail(type='text', placeholder='Email')
                      .control-group
                        input#inputPassword(type='password', placeholder='Password')
                      .control-group
                        label.checkbox
                          input(type='checkbox')
                          |  Remember me
                    button.btn.btn-success(type='submit') Sign in
                    button.btn(data-dismiss='modal', aria-hidden='true') Close

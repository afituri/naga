extends layout
block navList_And_photoSlide
block header
block navListVertical  
block navigation
block footer  
block content
  br
  .col-xs-12.col-sm-12.col-md-3.logo-holder.col-md-offset-1
    //- ============================================================= LOGO =============================================================
    .logo
      a(href='/')
        img(style='height:75px;margin-top:17px;',src="../../img/nagaLogo1.png")
  .row
  br
  .container
      .col-xs-12.no-margin
        .billing-address
          h2.border.h1 Register
          form
            .row.field-row
              .col-xs-12
                label email
                input.le-input
            // /.field-row
            .row.field-row
              .col-xs-12.col-sm-6
                label first name*
                input.le-input
              .col-xs-12.col-sm-6
                label last name*
                input.le-input
            // /.field-row
            
            .row.field-row
              .col-xs-12.col-sm-6
                label password
                input.le-input
              .col-xs-12.col-sm-6
                label confirm password
                input.le-input
            // /.field-row
            .row.field-row
              .col-xs-12.col-sm-6
                label home number
                input.le-input
              .col-xs-12.col-sm-6
                label mobile number
                input.le-input
            // /.field-row
            .row.field-row
              #create-account.col-xs-12
                input.le-checkbox.big(type='checkbox')
                a.simple-link.bold(href='#') Create Account?
                |  - you will receive email with temporary generated password after login you need to change it.
            br
            .row
              .col-xs-12.col-sm-6
                .buttons-holder
                  button.le-button.huge.col-xs-4(type='submit') Sign Up
            // /.field-row
        // /.billing-address
  // /.authentication



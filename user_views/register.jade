                    label.control-label.col-md-2.col-md-offset-2 Email
                    .col-md-6
                      .form-group
                        .col-md-11
                          input.form-control(id="email", type = "email", placeholder = "Email" , name ="email")
                            
                  .form-group    
                    label.control-label.col-md-2.col-md-offset-2  Password
                    .col-md-6
                      .form-group
                        .col-md-11
                          input.form-control#Password(name = "Password", type = "Password", placeholder = "Password")    
                      .form-group.internal
                        .col-md-11
                          input.form-control(id="confirmPassword",name="confirmPassword",type = "text", placeholder = "confirm Password")  
                  .form-group    
                    label.control-label.col-md-2.col-md-offset-2 Phone Namber
                    .col-md-6
                      .form-group
                        .col-md-11
                          input.form-control(name="Mobile",id="Mobile", type = "Namber", placeholder = "Mobile: {xxx} - xxx xxxx")
                      .form-group.internal
                        .col-md-11
                          input.form-control(name="HomePhone" , id="HomePhone" ,type= "Namber", placeholder = "HomePhone: {xxx} - xxx xxxx") 
